<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    
	<bean id="helloWorld"  class="com.cmcc.sunyiwei.springframework.HelloWorld" scope="prototype">
		<property name="name" value="sunyiwei" />
	</bean>
	
	<bean id="helloWorld_"  class="com.cmcc.sunyiwei.springframework.HelloWorld" scope="singleton">
		<property name="name" value="lisa" />
	</bean>
</beans>